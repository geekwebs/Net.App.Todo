{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Authorization": true,
  "IdentityBroker": {
    "EnableIdentityBroker": true,
    "JwtBearer": {
      "Authority": "http://localhost:8080/realms/netapp",
      "Audience": "account",
      "RequireHttpsMetadata": false
    }
  }
}
